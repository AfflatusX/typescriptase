{
  "name": "typescriptase",
  "version": "0.6.11",
  "description": "TypeScript Codegen",
  "main": "bin/index.js",
  "types": "bin/index.d.ts",
  "repository": "https://github.com/AriChivukula/typescriptase.git",
  "author": "Ari Chivukula <ari@chivuku.la>",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0-beta.51",
    "yargs": "11.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.51",
    "@babel/core": "7.0.0-beta.51",
    "@babel/preset-env": "7.0.0-beta.51",
    "@types/jest": "23.1.3",
    "@types/node": "10.5.0",
    "@types/yargs": "11.0.0",
    "jest": "23.2.0 ",
    "ts-jest": "22.4.6",
    "ts-node": "7.0.0",
    "typescript": "2.9.2"
  },
  "bin": {
    "typescriptase": "bin/cli.js"
  },
  "scripts": {
    "codegen": "ts-node src/cli.ts --files gen/*.ts",
    "build": "tsc && babel --minified bin/*.js -d bin/",
    "snap": "jest -u",
    "test": "jest && ts-node src/cli.ts --expectNoChanges --files gen/*.ts"
  }
}
